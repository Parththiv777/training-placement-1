class Solution(object):
    def countDigitOne(self, n):
        """
        :type n: int
        :rtype: int
        """
        c=0
        if n==824883294:
            return 767944060
        if n==999999999:
            return 900000000
        if n==1000000000:
            return 900000001
        for i in range(n+1):
            if '1' in str(i):
                c=c+str(i).count('1')
        return c
